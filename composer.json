{
    "name": "p7v/todoist-api",
    "description": "A PHP client for the official Todoist REST API (v8)",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Alex Plekhanov",
            "email": "alex@plekhanov.us"
        }
    ],
    "require": {
        "php": "^7.0.4",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.3",
        "webmozart/assert": "^1.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5.13"
    },
    "autoload": {
        "psr-4": {
            "P7v\\Todoist\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "minimum-stability": "stable",
    "config": {
        "sort-packages": true
    }
}
